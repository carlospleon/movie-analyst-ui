pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  environment {
    DOCKERHUB_CREDENTIALS = credentials('dockerhub')
  }
  stages {
    stage('Build') {
      steps {
        sh '
        echo '#!/bin/bash' > kube.sh
        echo 'sudo kubectl apply -f deploy-fe.yaml' >> kube.sh
        '
        sh 'ls'
      }
    }
  }
}
